{
    "expo": {
        "name": "Shubhkamna",
        "slug": "shubhkamna-ventures",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.shubhkamnaventures.shubhkamna",
            "buildNumber": "1.0.0"
        },
        "android": {
            "package": "com.shubhkamnaventures.shubhkamna",
            "versionCode": 1,
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#FFFFFF"
            },
            "permissions": [
                "INTERNET"
            ]
        },
        "androidStatusBar": {
            "backgroundColor": "#C60C30",
            "barStyle": "light-content",
            "translucent": false
        },
        "web": {
            "favicon": "./assets/favicon.png"
        }
    }
}
